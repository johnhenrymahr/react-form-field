{
    "extends": [
        "standard",
        "plugin:react/recommended",
        "prettier",
        "prettier/react",
        "prettier/standard"
    ],
    "plugins": [
        "babel",        
        "prettier",
        "react",
        "standard"
      ],
      "parserOptions": {
        "sourceType": "module",
        "ecmaFeatures": {
          "jsx": true
        }
      },
      "env": {
        "es6": true,
        "node": true
      },
      "rules": {
        "prettier/prettier": "error"
      }
    }
}